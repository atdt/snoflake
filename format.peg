// (1H0,I15,16H READS PERFORMED)

format
= '(' head:chunk tail:(',' chunk)* ')' end

chunk
= prefix:[0-9]* code:[A-Z] suffix:[0-9]* string:[^,)]*
{
  prefix = prefix.length ? parseInt(prefix, 10) : null;
  suffix = suffix.length ? parseInt(suffix, 10) : null;
  string = string.join('');
  return {
    prefix: prefix,
    code: code,
    suffix: suffix,
    string: string
  }
}

end
= !.
